{
  "name": "react-ref",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "lint": "./node_modules/.bin/eslint --fix \"./src/**/*.{ts,tsx,js,jsx}\"",
    "prettier": "prettier --write . \"./src/**/*.{ts,tsx,js,jsx}\"",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --coverage --watchAll false",
    "eject": "react-app-rewired eject"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "axios": "^0.21.0",
    "date-fns": "^2.16.1",
    "lodash": "^4.17.20",
    "node-sass": "^5.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.12.2",
    "react-icons": "^4.1.0",
    "react-lottie": "^1.2.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "source-map-explorer": "^2.5.1",
    "styled-components": "^5.2.1",
    "styled-theming": "^2.2.0",
    "typescript": "^4.1.2",
    "web-vitals": "^0.2.4",
    "yup": "^0.32.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/react-hooks": "^3.5.0",
    "@testing-library/user-event": "^12.5.0",
    "@types/axios": "^0.14.0",
    "@types/jest": "^26.0.16",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-lottie": "^1.2.5",
    "@types/react-router-dom": "^5.1.6",
    "@types/styled-components": "^5.1.4",
    "@types/styled-theming": "^2.2.5",
    "@types/yup": "^0.29.10",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "axios-mock-adapter": "^1.19.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-root-import": "^6.6.0",
    "customize-cra": "^1.0.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^4.3.4",
    "jest-canvas-mock": "^2.3.0",
    "lint-staged": "^10.5.3",
    "prettier": "2.2.1",
    "pretty-quick": "^3.1.0",
    "react-app-rewired": "^2.1.7",
    "react-test-renderer": "^17.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier && npm run lint",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "moduleNameMapper": {
      "\\.(woff2?|ttf|otf|eot|png|jpe?g|gif|svg|css|scss)$": "<rootDir>/mock/jest-mockfile.js",
      "^~\\/(.*)$": "<rootDir>/src/$1"
    },
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover",
      "html"
    ],
    "collectCoverageFrom": [
      "**/*.{tsx,jsx,ts}"
    ],
    "coveragePathIgnorePatterns": [
      "styles.ts",
      "global.ts",
      "theme.ts",
      "reportWebVitals.ts",
      "\\.d\\.ts$",
      "/src/assets/",
      "/node_modules/",
      "/src/index.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 95,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    }
  }
}
